            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="Initialize variables and gear the axis to the sawtooth signal."/>
               <Step>
                  <Expression
                      Src="Saw_Signal[_CurAxis] := Saw_ReqMinPos[_CurAxis];&#9;&#9;&#9;//Initialize the signal variable that the axis will be gearing to.&#13;&#10;Saw_CycleCount[_CurAxis] := 0;&#9;&#9;&#9;&#9;&#9;//Initialize cycle count.&#13;&#10;&#13;&#10;&#13;&#10;Fatigue_para[_CurAxis*5+0]:=0.0;&#13;&#10;&#13;&#10;Saw_Max_I_Term[_CurAxis] := 0.0;&#13;&#10;Saw_Min_I_Term[_CurAxis] := 0.0;&#9;&#13;&#10;&#13;&#10;&#13;&#10;CtrlOutputMax[_CurAxis]:=_Axis[].ControlOutput;&#13;&#10;CtrlOutputMin[_CurAxis]:=_Axis[].ControlOutput;"/>
                  <Expression
                      Src="//Initializations&#13;&#10;Saw_LastMax[_CurAxis] := _Axis[].ActPos; &#13;&#10;Saw_LastMin[_CurAxis] := _Axis[].ActPos;&#13;&#10;Saw_TarMax[_CurAxis] := Saw_ReqMaxPos[_CurAxis];&#13;&#10;Saw_TarMin[_CurAxis] := Saw_ReqMinPos[_CurAxis];&#13;&#10;Saw_LastTarMax[_CurAxis] := Saw_TarMax[_CurAxis];&#13;&#10;Saw_LastTarMin[_CurAxis] := Saw_TarMin[_CurAxis];&#13;&#10;&#13;&#10;Saw_PosTime[_CurAxis] := ( 1.0 - Saw_ReqDutyCycle[_CurAxis] ) / Saw_ReqFreq[_CurAxis];&#9;&#9;&#9;&#9;&#9;//Time to move from min to max position.&#13;&#10;Saw_NegTime[_CurAxis] := Saw_ReqDutyCycle[_CurAxis] / Saw_ReqFreq[_CurAxis];&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;//Time to move from max to min position.&#13;&#10;Saw_PosInc[_CurAxis] := _LoopTime * (Saw_TarMax[_CurAxis] - Saw_TarMin[_CurAxis]) / Saw_PosTime[_CurAxis];&#9;&#9;//Rate of move in positive direction.&#13;&#10;Saw_NegInc[_CurAxis] := _LoopTime * (Saw_TarMax[_CurAxis] - Saw_TarMin[_CurAxis]) / Saw_NegTime[_CurAxis];&#9;&#9;//Rate of move in negative direction."/>
                  <Command
