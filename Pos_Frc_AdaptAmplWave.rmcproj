                  <Expression
                      Src=""/>
               </Step>
               <Transition>
                  <Jump
                      Type="JUMP"
                      Next="Next"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP"
                Label="StartTrapWave_copy">
               <Comment
                   Comment="Move to lower extreme of the trapezoid wave."/>
               <Step>
                  <Command
                      Command="18"
                      CmdAxes="0"
                      CmdPrm1="Trap_Frc_ReqMinPos[_CurAxis]"
                      CmdPrm2="2.0"
                      CmdPrm3="5.0"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="IF-THEN"
                      Condition="_Axis[].StatusBits.AtForce"
                      True="Next"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="Initialize variables and gear the axis to the trapezoid signal."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_Signal[_CurAxis] := Trap_Frc_ReqMinPos[_CurAxis];&#9;&#9;&#9;//Initialize the signal variable that the axis will be gearing to.&#13;&#10;Trap_Frc_CycleCount[_CurAxis] := 0;&#9;&#9;&#9;&#9;&#9;//Initialize cycle count.&#13;&#10;&#13;&#10;Fatigue_para[_CurAxis*5+0]:=0.0;&#13;&#10;//END_IF&#13;&#10;&#13;&#10;Trap_Frc_Max_I_Term[_CurAxis] := 0.0;&#13;&#10;Trap_Frc_Min_I_Term[_CurAxis] := 0.0;&#9;&#13;&#10;&#13;&#10;Trap_Frc_Max_I_Term[_CurAxis] := 0.0;&#13;&#10;Trap_Frc_Min_I_Term[_CurAxis] := 0.0;&#9;"/>
                  <Expression
                      Src="//Initializations&#13;&#10;Trap_Frc_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;&#13;&#10;Trap_Frc_TarMax[_CurAxis] := Trap_ReqMaxPos[_CurAxis];&#13;&#10;Trap_Frc_TarMin[_CurAxis] := Trap_ReqMinPos[_CurAxis];&#13;&#10;&#13;&#10;Trap_Frc_PosInc[_CurAxis] := _LoopTime * (Trap_Frc_TarMax [_CurAxis]- Trap_Frc_TarMin[_CurAxis]) / Trap_Frc_RiseTime[_CurAxis];&#9;&#9;//Rate of move in positive direction.&#13;&#10;Trap_Frc_NegInc[_CurAxis] := _LoopTime * (Trap_Frc_TarMax[_CurAxis] - Trap_Frc_TarMin[_CurAxis]) / Trap_Frc_FallTime[_CurAxis];&#9;&#9;//Rate of move in negative direction."/>
                  <Command
                      Command="64"
                      CmdAxes="0"
                      CmdPrm1="10000.0"
                      CmdPrm2="1000000.0"
                      CmdPrm3="Seek (0)"/>
                  <Command
                      Command="59"
                      CmdAxes="0"
                      CmdPrm1="Trap_frc_Signal[_CurAxis]"
                      CmdPrm2="-100.0"
                      CmdPrm3="100.0"
                      CmdPrm4="-100.0"
                      CmdPrm5="100.0"
                      CmdPrm6="Truncate (1)"/>
               </Step>
               <Transition>
                  <Jump
                      Type="NEXT"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP"
                Label="TrapWaveCycle_copy">
               <Comment
                   Comment="Rising Section&#13;&#10;&#13;&#10;Increment the master position until we reach the maximum position.&#13;&#10;Once we reach the max position we go to the next step."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_Signal[_CurAxis] := SEL( Trap_Frc_Signal[_CurAxis] + Trap_Frc_PosInc[_CurAxis] &gt;= Trap_Frc_TarMax[_CurAxis], &#32;Trap_Frc_TarMax[_CurAxis], &#32;Trap_Frc_Signal[_CurAxis] + Trap_Frc_PosInc[_CurAxis] );&#13;&#10;&#13;&#10;//Min and max detection &#13;&#10;IF _Axis[].ActFrc &gt; Trap_Frc_LastMax[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ActFrc &lt; Trap_Frc_LastMin[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;&#13;&#10;END_IF&#13;&#10;&#13;&#10;IF _Axis[].ControlOutput&gt;CtrlOutputMax[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMax[_CurAxis] := _Axis[].ControlOutput; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ControlOutput&lt; CtrlOutputMin[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMin[_CurAxis]:= _Axis[].ControlOutput;&#13;&#10;END_IF"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="CASE-ELSE"
                      Condition="Trap_Frc_Signal[_CurAxis] &gt;= Trap_Frc_TarMax[_CurAxis]"
                      True="Next"
                      False="Repeat"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Step>
                  <Expression
                      Src="Trap_Frc_Timer[_CurAxis] := _SysMS;"/>
               </Step>
               <Transition>
                  <Jump
                      Type="NEXT"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="High Section&#13;&#10;&#13;&#10;Wait the time specified for staying at the maximum position."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_Signal[_CurAxis] := Trap_Frc_TarMax[_CurAxis];&#13;&#10;&#13;&#10;//Min and max detection &#13;&#10;IF _Axis[].ActFrc &gt; Trap_Frc_LastMax[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ActFrc &lt; Trap_Frc_LastMin[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;&#13;&#10;END_IF&#13;&#10;&#13;&#10;IF _Axis[].ControlOutput&gt;CtrlOutputMax[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMax[_CurAxis] := _Axis[].ControlOutput; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ControlOutput&lt; CtrlOutputMin[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMin[_CurAxis]:= _Axis[].ControlOutput;&#13;&#10;END_IF"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="CASE-ELSE"
                      Condition="DINT_TO_REAL(_SysMS - Trap_Frc_Timer[_CurAxis]) / 1000.0 &gt;= Trap_Frc_HighTime[_CurAxis] - 2.0* _LoopTime"
                      True="Next"
                      False="Repeat"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="Falling Section&#13;&#10;&#13;&#10;Here we decrement the master position until we reach the minimum position.&#13;&#10;Once we reach the min position we go to the next step."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_Signal[_CurAxis] := SEL( Trap_Frc_Signal[_CurAxis] - Trap_Frc_NegInc[_CurAxis] &lt;= Trap_Frc_TarMin[_CurAxis], &#32;Trap_Frc_TarMin[_CurAxis], &#32;Trap_Frc_Signal[_CurAxis] - Trap_NegInc[_CurAxis] );&#13;&#10;&#13;&#10;//Min and max detection &#13;&#10;IF _Axis[].ActFrc &gt; Trap_Frc_LastMax[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ActFrc &lt; Trap_Frc_LastMin[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;&#13;&#10;END_IF&#13;&#10;&#13;&#10;IF _Axis[].ControlOutput&gt;CtrlOutputMax[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMax[_CurAxis] := _Axis[].ControlOutput; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ControlOutput&lt; CtrlOutputMin[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMin[_CurAxis]:= _Axis[].ControlOutput;&#13;&#10;END_IF"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="CASE-ELSE"
                      Condition="Trap_Frc_Signal[_CurAxis] &lt;= Trap_Frc_TarMin[_CurAxis]"
                      True="Next"
                      False="Repeat"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Step>
                  <Expression
                      Src="Trap_Frc_Timer[_CurAxis] := _SysMS;"/>
               </Step>
               <Transition>
                  <Jump
                      Type="NEXT"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="Low Section&#13;&#10;&#13;&#10;Wait the time specified for staying at the minimum position."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_Signal[_CurAxis] := Trap_Frc_TarMin[_CurAxis];&#13;&#10;&#13;&#10;//Min and max detection &#13;&#10;IF _Axis[].ActFrc&gt; Trap_Frc_LastMax[_CurAxis] THEN&#13;&#10;&#9;Trap_Frc_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ActPos &lt; Trap_Frc_LastMin[_CurAxis] &#32;THEN&#13;&#10;&#9;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;&#13;&#10;END_IF&#13;&#10;&#13;&#10;IF _Axis[].ControlOutput&gt;CtrlOutputMax[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMax[_CurAxis] := _Axis[].ControlOutput; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ControlOutput&lt; CtrlOutputMin[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMin[_CurAxis]:= _Axis[].ControlOutput;&#13;&#10;END_IF&#13;&#10;"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="CASE-ELSE"
                      Condition="DINT_TO_REAL(_SysMS - Trap_Frc_Timer[_CurAxis]) / 1000.0 &gt;= Trap_Frc_LowTime[_CurAxis] - 2.0* _LoopTime"
                      True="Next"
                      False="Repeat"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP">
               <Comment
                   Comment="Do the adaptive control on the max and min positions.&#13;&#10;Do initializations for next cycle.&#13;&#10;Increment the cycle count and then check if enough cycles have been completed."/>
               <Step>
                  <Expression
                      Src="Trap_Frc_LastCycleMax[_CurAxis] := Trap_Frc_LastMax[_CurAxis];&#13;&#10;Trap_Frc_LastCycleMin[_CurAxis] := Trap_Frc_LastMin[_CurAxis];&#13;&#10;Fatigue_Read[_CurAxis*5+0]:=Trap_Frc_LastMax[_CurAxis];&#13;&#10;Fatigue_Read[_CurAxis*5+1]:=Trap_Frc_LastMin[_CurAxis];&#13;&#10;//END_IF&#13;&#10;IF (CtrlOutputMax[_CurAxis]&lt;85.0 OR CtrlOutputMin[_CurAxis]&gt;-85.0) OR &#13;&#10;Trap_Frc_LastMax[_CurAxis]&gt;Trap_Frc_ReqMaxPos[_CurAxis] OR&#13;&#10;Trap_Frc_LastMin[_CurAxis]&lt;Trap_Frc_ReqMinPos[_CurAxis] &#32;THEN&#13;&#10;//Do the adaptive control&#13;&#10;Trap_Frc_Max_I_Term[_CurAxis] := Trap_Frc_Max_I_Term[_CurAxis] + Trap_Frc_Max_I_Gain[_CurAxis] * (Trap_Frc_ReqMaxPos[_CurAxis] - Trap_Frc_LastMax[_CurAxis]);&#13;&#10;Trap_Frc_TarMax[_CurAxis] := Trap_Frc_ReqMaxPos[_CurAxis] + Trap_Frc_Max_I_Term[_CurAxis];&#13;&#10;Trap_Frc_Min_I_Term[_CurAxis] := Trap_Frc_Min_I_Term[_CurAxis] + Trap_Frc_Min_I_Gain[_CurAxis]* (Trap_Frc_ReqMinPos[_CurAxis] - Trap_Frc_LastMin[_CurAxis]);&#13;&#10;Trap_Frc_TarMin[_CurAxis] := Trap_Frc_ReqMinPos[_CurAxis] + Trap_Frc_Min_I_Term[_CurAxis];&#13;&#10;END_IF"/>
                  <Expression
                      Src="//Initializations for next cycle.&#13;&#10;Trap_Frc_LastMax[_CurAxis] := _Axis[].ActFrc; &#13;&#10;Trap_Frc_LastMin[_CurAxis] := _Axis[].ActFrc;"/>
                  <Expression
                      Src="Trap_Frc_CycleCount[_CurAxis] := Trap_Frc_CycleCount[_CurAxis] + 1;"/>
               </Step>
               <Transition>
                  <CheckCondition
                      Type="IF-THEN-ELSE"
                      Condition="&#32;(Fatigue_Read[_CurAxis*5+2]&gt;= DINT_TO_REAL(Trap_Frc_ReqCycles[_CurAxis]) ) &#32;"
                      True="Next"
                      False="TrapWaveCycle"/>
               </Transition>
            </ProgramBlock>
            <ProgramBlock
                Type="STEP"
                Label="StopTrapWave_copy">
               <Comment
                   Comment="Stop Gearing"/>
               <Step>
                  <Command
                      Command="5"
                      CmdAxes="0"
                      CmdPrm1="Standard (0)"/>
               </Step>
               <Transition>
                  <Jump
