                Label="SawToothCycle">
               <Comment
                   Comment="Here we increment the master position until we reach the maximum position.&#13;&#10;Once we reach the max position we go to the next step.&#13;&#10;"/>
               <Step>
                  <Expression
                      Src="Saw_Signal[_CurAxis] := SEL( Saw_Signal[_CurAxis] + Saw_PosInc[_CurAxis] &gt;= Saw_TarMax[_CurAxis], &#32;Saw_TarMax[_CurAxis], &#32;Saw_Signal[_CurAxis] + Saw_PosInc[_CurAxis]);&#13;&#10;&#13;&#10;//Min and max detection &#13;&#10;IF _Axis[].ActPos &gt; Saw_LastMax[_CurAxis] &#32;THEN&#13;&#10;&#9;Saw_LastMax[_CurAxis] := _Axis[].ActPos; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ActPos &lt; Saw_LastMin[_CurAxis] &#32;THEN&#13;&#10;&#9;Saw_LastMin[_CurAxis] := _Axis[].ActPos;&#13;&#10;END_IF&#13;&#10;&#13;&#10;IF _Axis[].ControlOutput&gt;CtrlOutputMax[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMax[_CurAxis] := _Axis[].ControlOutput; &#13;&#10;END_IF&#13;&#10;IF _Axis[].ControlOutput&lt; CtrlOutputMin[_CurAxis] &#32;THEN&#13;&#10;&#9;CtrlOutputMin[_CurAxis]:= _Axis[].ControlOutput;&#13;&#10;END_IF"/>
