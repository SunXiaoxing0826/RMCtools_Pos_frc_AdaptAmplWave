                   Comment="Do the adaptive control on the max and min positions.&#13;&#10;Do initializations for next cycle.&#13;&#10;Increment the cycle count and then check if enough cycles have been completed."/>
               <Step>
                  <Expression
                      Src="&#13;&#10;Fatigue_Read[_CurAxis*10+0]:=Saw_LastMax[_CurAxis];&#13;&#10;Fatigue_Read[_CurAxis*10+1]:=Saw_LastMin[_CurAxis];&#13;&#10;&#13;&#10;IF (CtrlOutputMax[_CurAxis]&lt;85.0 OR CtrlOutputMin[_CurAxis]&gt;-85.0) OR &#32;&#32;Saw_LastMax[_CurAxis]&gt; Saw_ReqMaxPos[_CurAxis] OR Saw_LastMin[_CurAxis]&gt; Saw_ReqMinPos[_CurAxis] &#32;THEN&#13;&#10;Saw_LastTarMax[_CurAxis] := Saw_TarMax[_CurAxis];&#13;&#10;Saw_LastTarMin[_CurAxis] := Saw_TarMin[_CurAxis];&#13;&#10;Saw_LastCycleMax[_CurAxis] := Saw_LastMax[_CurAxis];&#13;&#10;Saw_LastCycleMin[_CurAxis] := Saw_LastMin[_CurAxis];&#13;&#10;&#13;&#10;&#13;&#10;//Do the adaptive control&#13;&#10;Saw_Max_I_Term[_CurAxis] := Saw_Max_I_Term[_CurAxis] + Saw_Max_I_Gain[_CurAxis] * (Saw_ReqMaxPos[_CurAxis] - Saw_LastMax[_CurAxis]);&#13;&#10;Saw_TarMax[_CurAxis] := Saw_ReqMaxPos[_CurAxis] + Saw_Max_I_Term[_CurAxis];&#13;&#10;Saw_Min_I_Term[_CurAxis] := Saw_Min_I_Term[_CurAxis] + Saw_Min_I_Gain[_CurAxis]* (Saw_ReqMinPos[_CurAxis] - Saw_LastMin[_CurAxis]);&#13;&#10;Saw_TarMin[_CurAxis] := Saw_ReqMinPos[_CurAxis] + Saw_Min_I_Term[_CurAxis];&#13;&#10;END_IF"/>
                  <Expression
                      Src="//Initializations for next cycle.&#13;&#10;Saw_LastMax[_CurAxis] := _Axis[].ActPos; &#13;&#10;Saw_LastMin[_CurAxis] := _Axis[].ActPos;&#13;&#10;&#13;&#10;Saw_PosTime[_CurAxis] := ( 1.0 - Saw_ReqDutyCycle[_CurAxis] ) / Saw_ReqFreq[_CurAxis];&#9;&#9;&#9;&#9;&#9;//Time to move from min to max position.&#13;&#10;Saw_NegTime[_CurAxis] := Saw_ReqDutyCycle[_CurAxis] / Saw_ReqFreq[_CurAxis];&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;//Time to move from max to min position.&#13;&#10;Saw_PosInc[_CurAxis] := _LoopTime * (Saw_TarMax[_CurAxis] - Saw_LastTarMin[_CurAxis]) / Saw_PosTime[_CurAxis];&#9;&#9;//Rate of move in positive direction.&#13;&#10;Saw_NegInc[_CurAxis] := ABS(_LoopTime * (Saw_TarMin[_CurAxis] - Saw_TarMax[_CurAxis])) / Saw_NegTime[_CurAxis];&#9;&#9;//Rate of move in negative direction."/>
